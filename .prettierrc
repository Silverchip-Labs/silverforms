{
    "bracketSpacing": true,
    "printWidth": 100,
    "proseWrap": "never",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "tabWidth": 4,
    "parser": "babel",
    "trailingComma": "all",
    "useTabs": false,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "printWidth": 200
            }
        }
    ]
}